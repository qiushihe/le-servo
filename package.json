{
  "name": "le-servo",
  "version": "0.0.1",
  "description": "Simple Let's Encrypt Server",
  "main": "build/server/index.js",
  "scripts": {
    "eslint": "./node_modules/.bin/eslint src/** test/**",
    "build": "rm -fr build/server && ./node_modules/.bin/babel src --out-dir build/server",
    "build-test": "npm run build && rm -fr build/test && BABEL_ENV=test ./node_modules/.bin/babel test --out-dir build/test",
    "server": "npm run build && node ./build/server/index.js",
    "test": "npm run eslint && npm run build-test && ./node_modules/.bin/mocha --recursive build/test"
  },
  "repository": "https://github.com/qiushihe/le-servo",
  "author": "Billy He",
  "license": "UNLICENSED",
  "private": true,
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-provide-modules": "^1.0.3",
    "babel-plugin-require-root-rewrite": "^1.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "body-parser": "^1.17.2",
    "chai": "^3.5.0",
    "chai-change": "^2.1.2",
    "eslint": "^3.19.0",
    "mocha": "^3.3.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "sinon": "^2.2.0",
    "sinon-chai": "^2.10.0"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "express": "^4.15.2",
    "lodash": "^4.17.4",
    "node-jose": "^0.9.4",
    "uuid": "^3.0.1"
  }
}
